  "author": "Christian Pasin Rey",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/runtime-dom": "^3.5.18",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4"
  }
}
