{
  "name": "@christianpasinrey/vue3-debug-data",
  "version": "1.0.10",
  "description": "Visual debugging plugin for Vue 3.",
  "main": "dist/vue3-debug-data.umd.js",
  "module": "dist/vue3-debug-data.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vue3-debug-data.es.js",
      "require": "./dist/vue3-debug-data.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
